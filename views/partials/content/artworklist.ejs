
          <article class="sidebar">
            <h1 class="sidebar-title">Gallery</h1>
            <p class="sidebar-body">Checkout the gallery section for latest albumarts and band pictures.</p>
            <div class="pixgrid clearfix" >
              <% if(artwork.length > 0) {
                 for(i=0; i< artwork.length; i++) { %>
                  <a href="/images/bands/<%= artwork[i] %>" data-toggle="lightbox">
                    <img  src='/images/bands/<%= artwork[i] %>' alt='Albumart <%= i %>' class="img-fluid">
                  </a>
                <% }
                  } %>

            </div>
          </article>
